; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Resource",
"class": "Background",
"language": "GDScript",
"path": "res://Backgrounds/Background.gd"
}, {
"base": "Resource",
"class": "Character",
"language": "GDScript",
"path": "res://Characters/Character.gd"
}, {
"base": "Node",
"class": "CharacterDisplayer",
"language": "GDScript",
"path": "res://Characters/CharacterDisplayer.gd"
} ]
_global_script_class_icons={
"Background": "",
"Character": "",
"CharacterDisplayer": ""
}

[application]

config/name="writing-for-godot"
run/main_scene="res://Main.tscn"
config/icon="res://icon.png"

[autoload]

ResourceDB="*res://Autoloads/ResourceDB.gd"

[editor_plugins]

enabled=PoolStringArray( "res://addons/paulloz.ink/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true

[ink]

inklecate_path="res://inklecate"

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
